{
  "name": "b2viewer",
  "version": "1.0.0",
  "author": "Shen-Yu <shenyu@hotmail.com>",
  "license": "GPL-3.0",
  "scripts": {
    "start": "set PORT=3003&&umi dev",
    "dev": "set PORT=3003&&umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "homepage": "https://github.com/Shen-Yu/b2-viewer",
  "repository": {
    "type": "git",
    "url": "git://github.com/Shen-Yu/b2-viewer.git"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.7",
    "antd": "^4.4.0",
    "copy-to-clipboard": "^3.3.1",
    "crypto-js": "^4.0.0",
    "hash.js": "^1.1.7",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "qier-player": "^1.2.10",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-responsive-carousel": "^3.2.9",
    "umi": "^3.2.7",
    "yorkie": "^2.0.0"
  },
  "__npminstall_done": false
}